{
  "apps": [
    {
      "name": "{{APP_NAME}}",
      "script": "npm",
      "args": "start",
      "instances": 1,
      "exec_mode": "fork",
      "env": {
        "NODE_ENV": "production",
        "USERNAME": "{{USERNAME}}",
        "FQDN": "{{FQDN}}",
        "APP_NAME": "{{APP_NAME}}",
        "PORT": "{{PORT}}"
      }
    }
  ]
}
